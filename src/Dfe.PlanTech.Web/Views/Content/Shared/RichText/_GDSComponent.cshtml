@using Dfe.PlanTech.Domain.Content.Models.ContentSupport.Mapped.Types
@model Dfe.PlanTech.Domain.Content.Models.ContentSupport.Mapped.Custom.CustomComponent

@{
    switch (Model.Type)
    {
        case CustomComponentType.Attachment:
            <partial name="Shared/RichText/Custom/_Attachment" model="Model"/>
            break;
        case CustomComponentType.Accordion:
            <partial name="Shared/RichText/Custom/_Accordion" model="Model"/>
            break;
        case CustomComponentType.Card:
            <partial name="Shared/RichText/Custom/_Card" model="Model"/>
            break;
        case CustomComponentType.GridContainer:
            <partial name="Shared/RichText/Custom/_GridContainer" model="Model"/>
            break;
        default:
            <partial name="Shared/_UnsupportedElement" model="Model"/>
            break;
    }
}
