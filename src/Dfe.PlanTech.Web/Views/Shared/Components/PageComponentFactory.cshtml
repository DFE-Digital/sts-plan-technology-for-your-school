@using Dfe.PlanTech.Web.Helpers;

@inject ComponentViewsHelper ComponentViewsHelper;

@model Dfe.PlanTech.Domain.Content.Models.ContentComponent;

@{
	if (ComponentViewsHelper.TryGetViewForType(Model, out string? path) && !string.IsNullOrEmpty(path))
	{
        await Html.RenderPartialAsync(path, Model);
    }
}
